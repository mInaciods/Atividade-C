#include <stdio.h>
#include <stdlib.h>
#include <iostream>

int main(){
    float nota1, nota2, nota3, nota4;
 
    printf("nota1 = ");
    scanf("%f",&nota1);
    printf("nota2 = ");
    scanf("%f",&nota2);
    printf("nota3 = ");
    scanf("%f",&nota3);
	printf("nota4 = ");
    scanf("%f",&nota4);
    
    float total = nota1 + nota2 + nota3 + nota4;
    
    printf("Nota total: %f", total);
    
    return 0;
}
